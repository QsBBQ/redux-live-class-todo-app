{
  "name": "redux-live-class-todo-app",
  "version": "1.0.0",
  "description": "A minimal React+Redux example todo application",
  "scripts": {
    "start": "NODE_ENV=development node server.js",
    "test": "tape -r babel-register 'src/**/*spec.js' | faucet",
    "lint": "eslint src"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshdmiller/redux-live-class-todo-app.git"
  },
  "author": "Josh David Miller <josh@joshdavidmiller.com> (http://joshdavidmiller.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshdmiller/redux-live-class-todo-app/issues"
  },
  "homepage": "https://github.com/joshdmiller/redux-live-class-todo-app",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "enzyme": "^2.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "faucet": "0.0.1",
    "react-addons-test-utils": "^15.2.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "tape": "^4.6.0",
    "webpack": "^2.1.0-beta.14",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
